pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "Building main branch..." '
            }
        }
        
        stage('Test') {
            steps {
                sh 'echo "Testing main branch..." '
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'echo "Deploying main branch..." '
            }
        }
    }
}