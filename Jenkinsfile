pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "Building dev branch..." '
            }
        }
        
        stage('Test') {
            steps {
                sh 'echo "Testing dev branch..." '
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'echo "Deploying dev branch..." '
            }
        }
    }
}